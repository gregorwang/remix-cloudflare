{
  "name": "my-remix-app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "dev:worker": "npm run build && wrangler dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler dev",
    "typecheck": "tsc",
    "cf:login": "wrangler login",
    "deploy": "npm run build && wrangler deploy",
    "d1:migrate:local": "wrangler d1 migrations apply app-db --local",
    "d1:migrate:remote": "wrangler d1 migrations apply app-db --remote"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@remix-run/cloudflare": "^2.17.4",
    "@remix-run/react": "^2.17.4",
    "@types/nodemailer": "^7.0.3",
    "better-auth": "^1.3.34",
    "clsx": "^2.1.1",
    "framer-motion": "^12.18.1",
    "isbot": "^4.1.0",
    "kysely": "^0.27.4",
    "kysely-d1": "^0.4.0",
    "nodemailer": "^7.0.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "resend": "^6.4.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260228.0",
    "@remix-run/dev": "^2.17.4",
    "@tailwindcss/postcss": "^4.1.10",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^4.1.10",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5.1.6",
    "vite": "^6.0.0",
    "vite-tsconfig-paths": "^4.2.1",
    "wrangler": "^3.114.17"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
